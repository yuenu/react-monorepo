    "@types/testing-library__cypress": "^5.0.9",
    "eslint-config-custom": "*",
    "jsdom": "^21.1.0",
    "prettier": "latest",
    "turbo": "latest",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5",
    "vitest-axe": "^0.1.0",
